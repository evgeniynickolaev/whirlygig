set -tmpdir "xst/projnav.tmp"
set -xsthdpdir "xst"
run
-ifn whirlygig.prj
-ifmt mixed
-ofn whirlygig
-ofmt NGC
-p xc9500xl
-top whirlygig
-opt_mode Speed
-opt_level 1
-iuc NO
-lso whirlygig.lso
-keep_hierarchy YES
-netlist_hierarchy as_optimized
-rtlview Yes
-hierarchy_separator /
-bus_delimiter <>
-case maintain
-verilog2001 YES
-fsm_extract YES -fsm_encoding Auto
-safe_implementation No
-vlgcase Full-Parallel
-mux_extract YES
-resource_sharing YES
-iobuf YES
-pld_mp YES
-pld_xp YES
-pld_ce YES
-wysiwyg NO
-equivalent_register_removal YES
